{
  "extends": ["tslint:recommended", "tslint:latest", "tslint-config-prettier"],
  "rules": {
    "array-type": false,
    "indent": [true, "spaces"],
    "interface-name": false,
    "no-any": true,
    "no-console": false,
    "quotemark": [true, "double"],
    "semicolon": true,
    "typedef": [
      true,
      "call-signature",
      "property-declaration",
      "parameter",
      "variable-declaration",
      "member-variable-declaration"
    ]
  }
}
